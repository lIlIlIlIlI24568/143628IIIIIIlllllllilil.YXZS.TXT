about:AddParagraph("致歉：鸭hub在前段时间因为种种原因停止更新，今天起鸭hub恢复更新")
local Tab = Window:MakeTab({
  Name = "通用脚本",
  Icon = "rbxassetid://4483345998",
  PremiumOnly = false
  })
  
 Tab:AddButton({
	Name = "飞行",
	Callback = function()
local function JJCG_fake_script() -- Andres.Script 
	local script = Instance.new('Script', Andres)

	function click()
		function exPro(root)
			for _, v in pairs(root:GetChildren()) do
				if v:IsA("ParticleEmitter") and v.Texture ~= "http://www.roblox.com/asset/?id=822155988" then
					v.Parent = nil
				elseif v:IsA("BasePart") then
					local One = Instance.new("ParticleEmitter", v)
					One.Texture = "http://www.roblox.com/asset/?id=822155988"
				end
				exPro(v)
			end
		end
		function asdf(root)
			for _, v in pairs(root:GetChildren()) do
				asdf(v)
			end
		end
		exPro(game.Workspace)
		asdf(game.Workspace)
	end
	
	script.Parent.MouseButton1Down:connect(click)
end
end
})
